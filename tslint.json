{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["**/*.json"]
  },
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": false,
    "member-access": false,
    "member-ordering": [true, { "order": "fields-first" }],
    "max-classes-per-file": false
  },
  "rulesDirectory": []
}
